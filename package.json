  "scripts": {
    "build": "turbo run build:esm build:lib build:types",
    "check": "turbo run lint test",
    "clean": "del '{e2e,examples,packages}/*/{coverage,dist,esm,lib,types,tsconfig.tsbuildinfo}'",
