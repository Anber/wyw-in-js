    "build:types": "tsc",
    "lint": "eslint --ext .js,.ts .",
    "test": "jest --config ./jest.config.js"
  },
  "wyw-in-js": {
    "tags": {
      "makeStyles": "./src/processors/makeStyles.ts"
    }
  }
